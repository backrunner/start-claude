{
  "name": "start-claude",
  "type": "module",
  "version": "0.1.2",
  "packageManager": "pnpm@10.14.0",
  "description": "Start your claude code with specified config friendly :D",
  "author": "BackRunner",
  "license": "MIT",
  "homepage": "https://github.com/backrunner/start-claude",
  "repository": {
    "type": "git",
    "url": "https://github.com/backrunner/start-claude.git"
  },
  "keywords": [
    "claude",
    "cli tool"
  ],
  "main": "./bin/cli.cjs",
  "module": "./bin/cli.mjs",
  "bin": {
    "start-claude": "./bin/cli.mjs",
    "sc": "./bin/cli.mjs"
  },
  "files": [
    "bin"
  ],
  "scripts": {
    "build": "npm run build:manager && rimraf ./bin && rollup -c rollup.config.ts --configPlugin typescript",
    "build:manager": "cd src/manager && npm run build",
    "watch": "rollup -c rollup.config.ts --configPlugin typescript --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:inspect": "pnpm dlx @eslint/config-inspector",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.859.0",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "inquirer": "^12.7.0",
    "next": "^15.4.6",
    "open": "^10.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.17.0",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/inquirer": "^8.2.11",
    "eslint": "^9.31.0",
    "eslint-plugin-format": "^1.0.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.45.0",
    "rollup-plugin-copy": "^3.5.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
