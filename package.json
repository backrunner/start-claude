{
  "name": "start-claude",
  "type": "module",
  "version": "0.2.2",
  "packageManager": "pnpm@10.14.0",
  "description": "Start your claude code with specified config friendly :D",
  "author": "BackRunner",
  "license": "MIT",
  "homepage": "https://github.com/backrunner/start-claude",
  "repository": {
    "type": "git",
    "url": "https://github.com/backrunner/start-claude.git"
  },
  "keywords": [
    "claude",
    "cli tool"
  ],
  "main": "./bin/cli.cjs",
  "module": "./bin/cli.mjs",
  "bin": {
    "start-claude": "./bin/cli.mjs",
    "sc": "./bin/cli.mjs"
  },
  "files": [
    "bin"
  ],
  "workspaces": [
    "src/manager"
  ],
  "scripts": {
    "build": "node --experimental-strip-types scripts/clean.ts && pnpm run build:parallel && pnpm run postbuild",
    "build:parallel": "concurrently \"pnpm run build:manager\" \"pnpm run build:cli\"",
    "postbuild": "node --experimental-strip-types scripts/postbuild.ts",
    "build:manager": "pnpm --filter manager build",
    "build:cli": "rollup -c rollup.config.ts --configPlugin typescript",
    "watch": "rollup -c rollup.config.ts --configPlugin typescript --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:inspect": "pnpm dlx @eslint/config-inspector",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "boxen": "^8.0.1",
    "chalk": "^5.5.0",
    "commander": "^13.1.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.6",
    "inquirer": "^12.9.1",
    "next": "^15.4.6",
    "open": "^10.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.19.0",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/inquirer": "^8.2.12",
    "@types/uuid": "^10.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.0",
    "eslint": "^9.33.0",
    "eslint-plugin-format": "^1.0.1",
    "prettier": "^3.6.2",
    "rollup": "^4.46.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
