{
  "name": "@start-claude/migrator",
  "version": "0.6.2",
  "private": true,
  "description": "Config migration package for start-claude",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "test": "vitest run",
