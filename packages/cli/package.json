{
  "name": "@start-claude/cli",
  "type": "module",
  "version": "0.5.0",
  "private": true,
  "description": "Start Claude CLI main package",
  "main": "src/cli/main.ts",
  "scripts": {
    "build": "rollup -c ../../rollup.config.ts --configPlugin typescript",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --config ../../eslint.config.js"
  },
  "dependencies": {
    "@start-claude/migrator": "workspace:*"
  },
  "devDependencies": {
    "eslint": "^9.33.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
